<%= flash[:success] %>
<h2><%= @project.title %> &nbsp;&nbsp;&nbsp;</h2>
<p><%= @project.description %></p>
</br>
<div class="btn-group">
    <%= link_to "Back", projects_path, class: "btn btn-default" %> 
    <%= link_to "Edit", edit_project_path(@project), class: "btn btn-info" %> 
    <%= link_to "Delete", project_path(@project), method: :delete, data: {confirm: "Are you sure you want to delete this project?"}, class: "btn btn-danger" %>
</div>
</br></br></br>  
<%= if @project.admin_id == current_user.id then link_to "Add Users", show_and_select_user_path(@project), class: "btn btn-primary" end %> </br></br>
<%=link_to "Project Files", project_project_files_path(project_id = @project.id), class: "btn btn-primary" %></br></br>
<%=link_to "Project Questions", project_questions_path(project_id = @project.id), class: "btn btn-primary" %>